{
  "name": "chatroom-spike",
  "version": "0.0.0",
  "description": "spike for the chatroom module of a social media website",
  "main": "./server/server.js",
  "scripts": {
    "server": "node ./server/server.js",
    "help": "printf \"help\"",
    "start": "npm run prod",
    "dev": "cross-env NODE_ENV=\"development\"; npm-run-all server",
    "prod": "cross-env NODE_ENV=\"production\"; npm-run-all server",
    "watch:dev": "nodemon"
  },
  "type": "module",
  "engines": {
    "node": ">=18"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/olsonadr/chatroom-spike.git"
  },
  "author": "Nicholas Olson, Kyle Kanwischer",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/olsonadr/chatroom-spike/issues"
  },
  "homepage": "https://github.com/olsonadr/chatroom-spike#readme",
  "dependencies": {
    "bcrypt": "^5.1.1",
    "body-parser": "^1.19.0",
    "bufferutil": "^4.0.1",
    "cookie-parser": "^1.4.4",
    "cross-env": "^7.0.0",
    "express": "^4.17.1",
    "express-handlebars": "^8.0.1",
    "express-session": "^1.17.0",
    "moment": "^2.24.0",
    "nodemon": "^3.1.9",
    "npm-run-all": "^4.1.5",
    "pg": "^8.13.3",
    "public-ip": "^7.0.1",
    "regenerator-runtime": "^0.14.1",
    "rimraf": "^6.0.1",
    "sequelize": "^6.37.5",
    "socket.io": "^4.8.1",
    "utf-8-validate": "^6.0.5",
    "webpack": "^5.98.0"
  },
  "nodemonConfig": {
    "exec": "npm run nobabel",
    "watch": [
      "server/*",
      "public/*",
      "views/*"
    ],
    "ignore": [
      "**/__tests__/**",
      "*.test.js",
      "*.spec.js"
    ]
  },
  "babel": {
    "presets": [
      "@babel/preset-env"
    ],
    "plugins": [
      "@babel/plugin-transform-spread",
      [
        "@babel/plugin-syntax-decorators",
        {
          "legacy": false,
          "decoratorsBeforeExport": true
        }
      ]
    ]
  },
  "devDependencies": {
    "@babel/cli": "^7.26.4",
    "@babel/node": "^7.26.0",
    "@babel/plugin-syntax-decorators": "^7.25.9",
    "@babel/plugin-transform-spread": "^7.25.9",
    "@babel/preset-env": "^7.26.9"
  }
}